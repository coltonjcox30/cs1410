class Account:
    def __init__(self,acc_name,acc_num,start_b):
        self.acc_name = account_holders_name
        self.acc_num = account_number
        self.start_b = starting_balance

    def deposit_Money(self,value):
        if value >= 0:
            self.start_b += value
            return True
        else:
            return False

    def withdrawl_Money(self,value):
        self.start_b -= value